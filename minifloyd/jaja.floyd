~ CBasics.floyd
~ - Tests C-level parameter passing and variable access
~ - Expected output: 0 10 20 0 10 -5 0 10 20 -5 25

class CBasics is
  d: int  
  meth1(a: int; b: int): int is
    c: int
    d: int
  begin
      c := 5
      d := 2
      meth1 := c
      meth1 := d
  end meth1

  start() is
    num: int
  begin 
    num := meth1(1,2)
    out.writeint(num)
  end start
end CBasics

